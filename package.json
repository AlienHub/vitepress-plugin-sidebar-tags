{
  "name": "vitepress-plugin-sidebar-tags",
  "version": "0.1.1",
  "description": "A powerful VitePress plugin to automatically add diverse tags to sidebar based on frontmatter",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css",
    "./client": {
      "import": "./dist/client.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CSS-INTEGRATION.md"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "prepublishOnly": "npm run build",
    "test": "node debug-helper.cjs"
