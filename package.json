{
  "name": "vitepress-plugin-sidebar-tags",
  "version": "0.2.2",
  "description": "A VitePress plugin to automatically add tags to sidebar based on frontmatter",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css",
    "./client": {
      "import": "./dist/client.mjs",
      "require": "./dist/client.js",
      "types": "./dist/client.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "node test/plugin.test.js"
  },
  "keywords": [
    "vitepress",
    "plugin",
    "sidebar",
    "tags",
    "frontmatter",
    "markdown",
    "documentation"
  ],
  "author": "AlienHub",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlienHub/vitepress-plugin-sidebar-tags.git"
  },
  "bugs": {
    "url": "https://github.com/AlienHub/vitepress-plugin-sidebar-tags/issues"
  },
  "homepage": "https://github.com/AlienHub/vitepress-plugin-sidebar-tags#readme",
  "peerDependencies": {
    "vitepress": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitepress": "^1.0.0"
  }
} 