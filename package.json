  "repository": {
    "type": "git",
    "url": "git@github.com:AlienHub/vitepress-plugin-sidebar-tags.git"
  },
  "homepage": "https://github.com/AlienHub/vitepress-plugin-sidebar-tags#readme",
  "bugs": {
    "url": "https://github.com/AlienHub/vitepress-plugin-sidebar-tags/issues"
  },
  "peerDependencies": {
    "vitepress": "^1.0.0"
  },
  "dependencies": {
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitepress": "^1.0.0"
  },
  "engines": {
    "node": ">=16"
  }
